{
  "name": "node_test_app_01",
  "version": "0.0.1",
  "description": "Demo app using Node.js for educational purposes",
  "main": "./dist/server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && copyfiles -u 1 src/*.html dist/",
    "start": "npm run build && node ./dist/server.js",
    "dev": "nodemon"
  },
  "keywords": [
    "nodejs",
    "demo",
    "crypto",
    "dogecoin"
  ],
  "author": "Gabriel Trujillo <joaqt23@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "dogeaverage": "^0.1.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.9.1",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
